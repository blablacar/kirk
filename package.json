{
  "name": "@blablacar/ui-library",
  "version": "1.0.1",
  "description": "React component library",
  "main": "build/index.js",
  "homepage": "https://blablacar.github.io/ui-library",
  "repository": {
    "type": "git",
    "url": "git@github.com:blablacar/ui-library.git"
  },
  "scripts": {
    "start": "export NODE_PATH=src && start-storybook",
    "build": "rimraf build && tsc && babel build/src -x \".jsx\",\".js\" --ignore \"**/*.unit.jsx\",\"**/story.jsx\" --out-dir build && rimraf build/src && rimraf build/stories",
    "tslint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" --project tsconfig.json",
    "lint": "git diff --name-only --cached --diff-filter=ACMRTUXB -- \"src/*.ts\" \"src/*.tsx\" | xargs tslint",
    "prettier": "prettier --write '**/*.{ts,tsx,js,jsx}'",
    "prettier:ci": "prettier --list-different '**/*.{js,jsx}'",
    "test": "jest",
    "test:coverage": "jest --no-cache --coverage",
    "watch": "npm test -- --watch --onlyChanged",
    "kirk": "node ./bin/kirkInit.js",
    "bla": "bla",
    "prepublishOnly": "npm run tslint && npm run test && npm run build",
    "storybook-build": "export NODE_PATH=src && build-storybook -c .storybook -o storybook-build",
    "storybook-deploy": "npm run storybook-build && gh-pages -d storybook-build"
  },
  "author": "Blablacar front-end team",
  "keywords": [
    "blablacar",
    "design-system",
    "atomic-design",
    "ui-library",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@blablacar/cli": "0.1.2",
    "@storybook/addon-a11y": "5.0.1",
    "@storybook/addon-actions": "5.0.1",
    "@storybook/addon-info": "5.0.1",
    "@storybook/addon-knobs": "5.0.1",
    "@storybook/addon-viewport": "5.0.1",
    "@storybook/addons": "5.0.1",
    "@storybook/react": "5.0.1",
    "@types/enzyme": "3.9.0",
    "@types/jest": "24.0.11",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.isempty": "4.4.6",
    "@types/lodash.isequal": "4.5.5",
    "@types/lodash.isstring": "4.0.6",
    "@types/lodash.padstart": "4.6.6",
    "@types/react": "16.8.8",
    "@types/react-dates": "17.1.3",
    "@types/react-dom": "16.8.2",
    "@types/react-transition-group": "2.0.16",
    "@types/storybook__react": "4.0.1",
    "@types/styled-jsx": "2.2.8",
    "ansi": "0.3.1",
    "babel-jest": "24.5.0",
    "babel-plugin-module-resolver": "3.2.0",
    "caniuse-db": "1.0.30000946",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.0",
    "gh-pages": "2.0.1",
    "glob": "7.1.3",
    "husky": "1.3.1",
    "inquirer": "6.2.2",
    "jest": "24.5.0",
    "jsdom": "14.0.0",
    "jsdom-global": "3.0.2",
    "lodash.camelcase": "4.3.0",
    "lodash.template": "4.4.0",
    "onchange": "5.2.0",
    "path": "0.12.7",
    "pre-commit": "1.2.2",
    "prettier": "1.16.4",
    "react-test-renderer": "16.8.4",
    "rimraf": "2.6.3",
    "stylelint": "9.10.1",
    "tslint": "5.14.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "classcat": "3.2.5",
    "country-telephone-data": "0.6.0",
    "lodash.debounce": "4.0.8",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "lodash.isstring": "4.0.1",
    "lodash.kebabcase": "4.1.1",
    "moment": "2.22.2",
    "react": "16.8.4",
    "react-dates": "16.7.0",
    "react-dom": "16.8.4",
    "react-transition-group": "2.6.0",
    "styled-jsx": "3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "bla node && bla check && npm run lint",
      "pre-push": "npm test"
    }
  }
}
