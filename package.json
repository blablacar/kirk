{
  "name": "@blablacar/ui-library",
  "version": "43.1.0",
  "description": "BlaBlaCar React UI component library",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "homepage": "https://blablacar.github.io/ui-library",
  "repository": {
    "type": "git",
    "url": "git@github.com:blablacar/ui-library.git"
  },
  "engines": {
    "node": "^12.16.1",
    "npm": "^6.13.4"
  },
  "scripts": {
    "start": "export NODE_PATH=src && start-storybook -p 4000",
    "build": "rimraf build && tsc && npm run build-babel && npm run build-rename-jsx-map && npm run build-cleanup",
    "build-babel": "babel build/src --copy-files --out-dir build",
    "build-rename-jsx-map": "renamer --find jsx.map --replace js.map \"build/**\"",
    "build-cleanup": "rimraf build/src && del \"build/**/story.*\" && del \"build/**/*.unit.*\"",
    "eslint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.mdx\"",
    "prettier": "prettier --write '**/*.{ts,tsx}'",
    "prettier:ci": "prettier --list-different '**/*.{ts,tsx}'",
    "test": "jest",
    "test:coverage": "jest --no-cache --coverage",
    "watch": "npm test -- --watch --onlyChanged",
    "bla": "bla",
    "prepublishOnly": "npm run eslint && npm run test && npm run build",
    "storybook-build": "export NODE_PATH=src && build-storybook -c .storybook -o storybook-build",
    "storybook-deploy": "npm run storybook-build && gh-pages -d storybook-build",
    "publish-canary": "npm --no-git-tag-version version \"0.0.0-$(git rev-parse --short HEAD)\" && npm publish --tag canary",
    "generate": "node tools/generator.js"
  },
  "author": "@blablacar/spa",
  "keywords": [
    "blablacar",
    "design-system",
    "component-library",
    "ui-library",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.1",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-react-jsx": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@blablacar/cli": "1.2.0",
    "@storybook/addon-a11y": "6.1.11",
    "@storybook/addon-actions": "6.1.11",
    "@storybook/addon-docs": "6.1.11",
    "@storybook/addon-knobs": "6.1.11",
    "@storybook/addon-links": "6.1.11",
    "@storybook/addon-viewport": "6.1.11",
    "@storybook/addons": "6.1.11",
    "@storybook/react": "6.1.11",
    "@testing-library/jest-dom": "5.8.0",
    "@testing-library/react": "11.1.2",
    "@testing-library/react-hooks": "3.4.2",
    "@types/enzyme": "3.10.4",
    "@types/exenv": "1.2.0",
    "@types/jest": "24.0.23",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.isempty": "4.4.6",
    "@types/lodash.isequal": "4.5.5",
    "@types/lodash.isstring": "4.0.6",
    "@types/lodash.uniqueid": "4.0.6",
    "@types/react": "16.9.16",
    "@types/react-color": "3.0.1",
    "@types/react-dom": "16.9.4",
    "@types/react-test-renderer": "16.9.1",
    "@types/react-transition-group": "4.4.0",
    "@types/storybook__react": "5.2.1",
    "@types/styled-components": "4.1.8",
    "@types/uuid": "3.4.6",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-preset-react-app": "9.1.1",
    "caniuse-db": "1.0.30001016",
    "classcat": "4.0.2",
    "core-js": "3.7.0",
    "del-cli": "3.0.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "6.7.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest-dom": "2.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-mdx": "1.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-testing-library": "3.1.3",
    "fs-extra": "9.0.1",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "jest": "26.6.3",
    "jest-date-mock": "1.0.8",
    "jest-environment-jsdom-fifteen": "1.0.2",
    "jest-styled-components": "6.3.4",
    "jsdom": "14.0.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "10.2.1",
    "prettier": "1.19.1",
    "pretty-log": "0.1.0",
    "react": "16.9.0",
    "react-docgen-typescript-loader": "3.7.2",
    "react-dom": "16.9.0",
    "react-test-renderer": "16.9.0",
    "readline": "1.3.0",
    "renamer": "1.1.4",
    "replace": "1.2.0",
    "rimraf": "2.6.3",
    "storybook-addon-smart-knobs": "6.0.2",
    "storybook-readme": "5.0.9",
    "styled-components": "4.3.2",
    "ts-loader": "6.2.1",
    "typescript": "3.7.3"
  },
  "dependencies": {
    "country-telephone-data": "0.6.0",
    "exenv": "1.2.2",
    "focus-trap": "5.0.0",
    "lodash.debounce": "4.0.8",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "lodash.isstring": "4.0.1",
    "lodash.uniqueid": "4.0.1",
    "react-day-picker": "7.3.0",
    "react-transition-group": "4.4.1",
    "uuid": "3.3.3"
  },
  "peerDependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.3.2",
    "classcat": "4.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.mdx": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "bla check && lint-staged && tsc --noEmit",
      "pre-push": "npm run eslint && npm test"
    }
  }
}
